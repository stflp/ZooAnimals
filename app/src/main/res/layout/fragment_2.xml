<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context=".Fragment2"
    android:background="@color/white"
    android:id="@+id/fragment2">

    <TextView
        android:id="@+id/animalName"
        android:layout_width="409dp"
        android:layout_height="733dp"
        android:gravity="center"
        android:text="@string/animalname"
        android:textSize="24sp"
        tools:layout_conversion_absoluteHeight="32dp"
        tools:layout_conversion_absoluteWidth="226dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/animalContinent"
        android:layout_width="418dp"
        android:layout_height="829dp"
        android:gravity="center"
        android:text="@string/animalcontinent"
        android:textSize="16sp"
        tools:layout_conversion_absoluteHeight="22dp"
        tools:layout_conversion_absoluteWidth="192dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />
</FrameLayout>